<owl:Class rdf:ID="Repository">
  <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class" />
  <rdfs:label xml:lang="en">Repository</rdfs:label>
  <rdfs:comment xml:lang="en">A source of data and/or artifact publications.</rdfs:comment>
  <rdfs:subClassOf rdf:resource="#Stream"/>
  <owl:equivalentClass rdf:resource="http://www.wikidata.org/entity/Q5227240"/>
</owl:Class>

<rdf:Property rdf:ID="streamTitle">
  <rdfs:label xml:lang="en">streamTitle</rdfs:label>
  <rdfs:comment xml:lang="en">A title of the stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:subPropertyOf rdf:resource="http://xmlns.com/foaf/0.1/name"/>
  <rdfs:subPropertyOf rdf:resource="https://schema.org/name"/>
  <rdfs:subPropertyOf rdf:resource="http://www.wikidata.org/entity/P2561"/>
</rdf:Property>
<rdf:Property rdf:ID="primaryStreamTitle">
  <rdfs:label xml:lang="en">primaryStreamTitle</rdfs:label>
  <rdfs:comment xml:lang="en">The primary title of the stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:subPropertyOf rdf:resource="#streamTitle"/>
  <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/commonName"/>
</rdf:Property>
<rdf:Property rdf:ID="formerStreamTitle">
  <rdfs:label xml:lang="en">formerStreamTitle</rdfs:label>
  <rdfs:comment xml:lang="en">A former title of the stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <rdfs:subPropertyOf rdf:resource="#streamTitle"/>
</rdf:Property>

<rdf:Property rdf:ID="issn">
  <rdfs:label xml:lang="en">issn</rdfs:label>
  <rdfs:comment xml:lang="en">An International Standard Serial Number.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyUri"/>
  <rdfs:subPropertyOf rdf:resource="#identifier"/>
  <owl:equivalentProperty rdf:resource="http://purl.org/spar/datacite/issn"/>
  <owl:equivalentProperty rdf:resource="http://id.loc.gov/vocabulary/identifiers/issn"/>
</rdf:Property>
<rdf:Property rdf:ID="iso4">
  <rdfs:label xml:lang="en">iso4</rdfs:label>
  <rdfs:comment xml:lang="en">The stream's ISO4 abbreviation.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  <owl:equivalentProperty rdf:resource="http://www.wikidata.org/entity/P1160"/>
</rdf:Property>

<rdf:Property rdf:ID="indexPage">
  <rdfs:label xml:lang="en">indexPage</rdfs:label>
  <rdfs:comment xml:lang="en">The URL of the dblp stream index page for this stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Document"/>
</rdf:Property>

<rdf:Property rdf:ID="relatedStream">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdfs:label xml:lang="en">relatedStream</rdfs:label>
  <rdfs:comment xml:lang="en">This stream is related to the other stream in some unspecified way.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="#Stream"/>
</rdf:Property>
<rdf:Property rdf:ID="superStream">
  <rdfs:label xml:lang="en">superStream</rdfs:label>
  <rdfs:comment xml:lang="en">This stream has (or had) the other stream as a part.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="#Stream"/>
  <rdfs:subPropertyOf rdf:resource="#relatedStream"/>
  <owl:inverseOf rdf:resource="#subStream"/>
</rdf:Property>
<rdf:Property rdf:ID="subStream">
  <rdfs:label xml:lang="en">subStream</rdfs:label>
  <rdfs:comment xml:lang="en">This stream is (or was) a part of the other stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="#Stream"/>
  <rdfs:subPropertyOf rdf:resource="#relatedStream"/>
  <owl:inverseOf rdf:resource="#superStream"/>
</rdf:Property>
<rdf:Property rdf:ID="predecessorStream">
  <rdfs:label xml:lang="en">predecessorStream</rdfs:label>
  <rdfs:comment xml:lang="en">This stream is a predecessor of the other stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="#Stream"/>
  <rdfs:subPropertyOf rdf:resource="#relatedStream"/>
  <owl:inverseOf rdf:resource="#successorStream"/>
</rdf:Property>
<rdf:Property rdf:ID="successorStream">
  <rdfs:label xml:lang="en">successorStream</rdfs:label>
  <rdfs:comment xml:lang="en">This stream is a successor of the other stream.</rdfs:comment>
  <rdfs:domain rdf:resource="#Stream"/>
  <rdfs:range rdf:resource="#Stream"/>
  <rdfs:subPropertyOf rdf:resource="#relatedStream"/>
  <owl:inverseOf rdf:resource="#predecessorStream"/>
</rdf:Property>

